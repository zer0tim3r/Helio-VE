<?xml version="1.0" encoding="utf-8"?>
<direct>
  <rule ipv="ipv4" table="nat" chain="POSTROUTING" priority="0">-o enp7s0 -j MASQUERADE</rule>
  <rule ipv="ipv4" table="nat" chain="PREROUTING" priority="0">-d 169.254.169.254 -p tcp --dport 80 -j DNAT --to-destination 192.168.10.254:8180</rule>
  <rule ipv="ipv4" table="filter" chain="FORWARD" priority="1">'!' -s 192.168.10.0/24 -j ACCEPT</rule>
  <rule ipv="ipv4" table="filter" chain="FORWARD" priority="2">-j DROP</rule>
</direct>