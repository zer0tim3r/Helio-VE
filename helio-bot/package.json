{
  "name": "helio-bot",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "packageManager": "yarn@4.5.0",
  "scripts": {
    "build": "tsc -p . && tsc-alias",
    "build:proto": "yarn dlx protoc --ts_out ./src/proto --proto_path ../proto ../proto/rpc.proto",
    "start": "node ./dist/index.js | pino-pretty",
    "dev": "DEV=1 ts-node-dev --respawn --transpile-only -r tsconfig-paths/register ./src/index.ts | pino-pretty",
    "dev:test": "DEV=1 ts-node-dev --respawn --transpile-only -r tsconfig-paths/register ./src/test.ts | pino-pretty"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.12.2",
    "@protobuf-ts/grpc-transport": "^2.9.4",
    "@protobuf-ts/plugin": "^2.9.4",
    "@protobuf-ts/runtime": "^2.9.4",
    "@protobuf-ts/runtime-rpc": "^2.9.4",
    "redis": "^4.7.0",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@types/uuid": "^10",
    "@yarnpkg/pnpify": "^4.1.2",
    "pino-pretty": "^11.2.2",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.8.10",
    "tsconfig-paths": "^4.2.0",
    "typescript": "5.3.3"
  }
}
